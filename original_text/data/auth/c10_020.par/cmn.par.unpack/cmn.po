msgctxt "185224"
msgid "副長！"
msgstr ""

msgctxt "185368"
msgid ""
"アンタだって新選組を使って\n"
"のし上がりたいっつってただろ！？"
msgstr ""

msgctxt "185512"
msgid "なあ　今すぐ俺を行かせて……"
msgstr ""

msgctxt "185656"
msgid ""
"面白そうな話やな\n"
"左之助"
msgstr ""

msgctxt "185800"
msgid "せやけどそれ……"
msgstr ""

msgctxt "185944"
msgid "一番隊がやらせてもらうわ"
msgstr ""

msgctxt "186088"
msgid "総司"
msgstr ""

msgctxt "186232"
msgid "文句あってもワシは止められへんで"
msgstr ""

msgctxt "186376"
msgid "歳ちゃん"
msgstr ""

msgctxt "186520"
msgid "ちょ　ちょっと待ってくれよ沖田さん！"
msgstr ""

msgctxt "186664"
msgid ""
"一番隊がやるのは構わねえ\n"
"だけど　俺も一緒にやらせてくれ！！"
msgstr ""

msgctxt "186808"
msgid "俺とアンタで一緒に　源さんの仇を！"
msgstr ""

msgctxt "186952"
msgid "なあ？　いいだろ！？"
msgstr ""

msgctxt "187096"
msgid "安心せえ左之助ちゃん"
msgstr ""

msgctxt "187240"
msgid ""
"誰もお前のことを\n"
"幕府の“歩”やなんて思てへん"
msgstr ""

msgctxt "187384"
msgid "はあ？"
msgstr ""

msgctxt "187528"
msgid "お前は"
msgstr ""

msgctxt "187672"
msgid "新選組の“歩”や"
msgstr ""

msgctxt "187816"
msgid "そこんとこよう　覚えとくんやな"
msgstr ""

msgctxt "187960"
msgid "一番隊！"
msgstr ""

msgctxt "188104"
msgid "行くで"
msgstr ""

msgctxt "188248"
msgid "はい！"
msgstr ""
