msgctxt "141696"
msgid "俺の本当の名は"
msgstr ""

msgctxt "141840"
msgid "“坂本龍馬”なんだ"
msgstr ""

msgctxt "141984"
msgid "え　それって！"
msgstr ""

msgctxt "142128"
msgid "一（はじめ）さんが　坂本龍馬？"
msgstr ""

msgctxt "142272"
msgid ""
"でも　それやったら\n"
"うちが会うた“坂本”さんは！？"
msgstr ""

msgctxt "142416"
msgid "そいつは俺の名前を騙っている偽者だ"
msgstr ""

msgctxt "142560"
msgid ""
"俺の恩人を暗殺させた黒幕で\n"
"口封じにその刺客を殺した"
msgstr ""

msgctxt "142704"
msgid ""
"長崎ではイギリスの武器商人と\n"
"つながっているらしい"
msgstr ""

msgctxt "142848"
msgid "そして……"
msgstr ""

msgctxt "142992"
msgid ""
"武市半平太と\n"
"同じ火傷を背中に背負っていた"
msgstr ""

msgctxt "143136"
msgid "でも　武市さんって……"
msgstr ""

msgctxt "143280"
msgid "わからねえ"
msgstr ""

msgctxt "143424"
msgid "兄弟は土佐で腹を切って死んだはずだ"
msgstr ""

msgctxt "143568"
msgid "それに……"
msgstr ""

msgctxt "143712"
msgid "あいつには親っさんを殺す理由もない"
msgstr ""

msgctxt "143856"
msgid "俺にももう……"
msgstr ""

msgctxt "144000"
msgid "何がどうなってんのか分からねえ……！"
msgstr ""

msgctxt "144144"
msgid "一（はじめ）さん"
msgstr ""

msgctxt "144288"
msgid "うちの会うた……"
msgstr ""

msgctxt "144432"
msgid "“坂本龍馬”に会いたい？"
msgstr ""

msgctxt "144576"
msgid "何？"
msgstr ""

msgctxt "144720"
msgid "もし会いたいんやったら"
msgstr ""

msgctxt "144864"
msgid ""
"新選組の人でうちと同じように\n"
"「坂本龍馬」とつながっている人がいるの"
msgstr ""

msgctxt "145008"
msgid "その人を捕まえれば……"
msgstr ""

msgctxt "145152"
msgid "もう一度“坂本龍馬”に会えるかも"
msgstr ""

msgctxt "145296"
msgid "誰なんだ？"
msgstr ""

msgctxt "145440"
msgid "その人は……"
msgstr ""

msgctxt "145584"
msgid "監察役　山崎烝"
msgstr ""

msgctxt "145728"
msgid "山崎烝"
msgstr ""

msgctxt "145872"
msgid "あいつが……？"
msgstr ""
