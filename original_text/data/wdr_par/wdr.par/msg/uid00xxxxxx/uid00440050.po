msgid ""
"し、信じられん……\n"
"なんなんだ、こいつ……"
msgstr ""

msgctxt "7907"
msgid "まだやるか……？"
msgstr ""

msgctxt "7924"
msgid "も……　も……"
msgstr ""

msgctxt "7939"
msgid "もうやりませぇぇぇぇん！"
msgstr ""

msgctxt "7964"
msgid "<!empty>"
