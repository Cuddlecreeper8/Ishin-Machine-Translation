msgctxt "4206"
msgid ""
"一（はじめ）ちゃん、\n"
"京の街でやり合うのも、最後になってもうたな。"
msgstr ""

msgctxt "4273"
msgid "そうだな……"
msgstr ""

msgctxt "4286"
msgid "土佐へ行く前に……　ワシとひと勝負せんか？"
msgstr ""

msgctxt "4329"
msgid ""
"なんや、一（はじめ）ちゃん。\n"
"また、ワシと勝負したいんか？"
msgstr ""

msgctxt "4388"
msgid "いや、遠慮しておこう……"
msgstr ""

msgctxt "4413"
msgid "なんや、そら残念やな……"
msgstr ""

msgctxt "4438"
msgid ""
"ワシはここにおるから、\n"
"いつでも勝負したかったら言ってや。"
msgstr ""

msgctxt "4497"
msgid "いいだろう……"
msgstr ""

msgctxt "4512"
msgid ""
"さっすが、一（はじめ）ちゃんや！\n"
"ワシも体がなまって、しょうがなかったんや！"
msgstr ""

msgctxt "4589"
msgid ""
"ほな、いくで！\n"
"一（はじめ）ちゃんよぉぉ！"
msgstr ""

msgctxt "4632"
msgid "<!empty>"
msgstr ""

msgctxt "4633"
msgid ""
"一（はじめ）ちゃん、\n"
"そろそろ、土佐へ出発する時間や。"
msgstr ""

msgctxt "4688"
msgid "もう、京の街で思い残すことは無いか？"
msgstr ""

msgctxt "4725"
msgid "いや……　まだだ。"
msgstr ""

msgctxt "4744"
msgid ""
"なら、はよう準備してや。\n"
"ワシら、ここで待っとるで。"
msgstr ""

msgctxt "4797"
msgid ""
"京の街で思い残すこと無いように、\n"
"しっかりケジメつけてくるんやで。"
msgstr ""

msgctxt "4864"
msgid "ああ……　大丈夫だ。"
msgstr ""

msgctxt "4885"
msgid ""
"よっしゃ、役者も全員揃ったようやし、\n"
"みんなで土佐へ殴り込みや！"
msgstr ""

msgctxt "4950"
msgid "<!empty>"
msgstr ""

msgctxt "4951"
msgid ""
"さすが、一（はじめ）ちゃんや！\n"
"また一段と強うなったんとちゃうか？"
msgstr ""

msgctxt "5018"
msgid "ふ……　そうかもな……"
msgstr ""

msgctxt "5041"
msgid ""
"ワシと勝負したかったら、いつでも言ってや。\n"
"相手になるで。"
msgstr ""

msgctxt "5100"
msgid "<!empty>"
msgstr ""

msgctxt "5101"
msgid ""
"どうしたん、一（はじめ）ちゃん？\n"
"今日は調子でも悪いんか？"
msgstr ""

msgctxt "5160"
msgid "<!empty>"
msgstr ""

msgctxt "5161"
msgid ""
"一（はじめ）ちゃん、\n"
"おりょうちゃんとのお別れは済んだんか？"
msgstr ""

msgctxt "5222"
msgid "いや……　まだだ。"
msgstr ""

msgctxt "5241"
msgid ""
"んなら、はよう行ってこいや。\n"
"男やったら、最後のケジメ、つけてこなアカンで。"
msgstr ""

