msgid "<!empty>"
msgstr ""

msgctxt "38017"
msgid "<!empty>"
msgstr ""

msgctxt "38018"
msgid "<!empty>"
msgstr ""

msgctxt "38019"
msgid "<!empty>"
msgstr ""

msgctxt "38020"
msgid ""
"いいだろう。\n"
"ついて来い、道場へ行くぞ。"
msgstr ""

msgctxt "38061"
msgid "ハイ、ヨロシクオネガイシマス！"
msgstr ""

