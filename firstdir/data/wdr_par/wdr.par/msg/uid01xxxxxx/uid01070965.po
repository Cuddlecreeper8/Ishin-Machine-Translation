msgctxt "2192"
msgid "まずは親を決めようか。"
msgstr "Let's decide the parents first."

msgctxt "2215"
msgid "俺が親だね。"
msgstr "I'm a parent."

msgctxt "2228"
msgid "（いくらで勝負する？）"
msgstr "(How much will you play?)"

msgctxt "2251"
msgid "さぁ、始めるよ。"
msgstr "Let's get started."

msgctxt "2268"
msgid "お、俺の番だな。"
msgstr "Oh, it's my turn."

msgctxt "2285"
msgid "もう一回振るぜ。"
msgstr "Shake it again."

msgctxt "2302"
msgid "精算だ。"
msgstr "It's a settlement."

msgctxt "2311"
msgid "（お前が親）"
msgstr "(You are the parent)"

msgctxt "2324"
msgid "親を交代しようか。"
msgstr "Shall we change parents?"

msgctxt "2343"
msgid "（もう一丁？）"
msgstr "(Another one?)"

msgctxt "2358"
msgid "（オケラ）"
msgstr "(Mole cricket)"

msgctxt "2369"
msgid "（出直して）"
msgstr "(Restart)"

msgctxt "2382"
msgid "親の即精算メッセージ。"
msgstr "Parent's immediate settlement message."

msgctxt "2405"
msgid "親の奇の見は３になるんだよねぇ。"
msgstr "The strange look of parents will be 3."

msgctxt "2438"
msgid "最初の子が出した奇の見は３になるんだよねぇ。"
msgstr ""
"The strange look that the first child gave\n"
"is 3, isn't it?"

msgctxt "2483"
msgid "奇の見は一つ前の子と同じ出目になるんだよねぇ。"
msgstr ""
"The strange look will be the same as the\n"
"previous child."

msgctxt "2530"
msgid "親の偶の音は４になるんだよねぇ。"
msgstr "The sound of the parent's even is 4."

msgctxt "2563"
msgid "偶の音は一つ後の子と同じ出目になるんだよねぇ。"
msgstr ""
"The sound of the even is the same as the\n"
"one after the next child, isn't it?"

msgctxt "2610"
msgid "最後の子が出した偶の音は４に化けるんだよねぇ。"
msgstr ""
"The even sound made by the last child turns\n"
"into 4."

msgctxt "2657"
msgid "偶の音は、後ろの子と同じ出目になる。"
msgstr ""
"The sound of the even is the same as the\n"
"child behind."

msgctxt "2694"
msgid "偶の音のあとに奇の見がでた。"
msgstr ""
"There was a strange look after the sound\n"
"of the even."

msgctxt "2723"
msgid "ムサシ！　ツイてるねぇ。"
msgstr "Musashi! It's awesome."

msgctxt "2752"
msgid "秋山"
msgstr "Akiyama"
