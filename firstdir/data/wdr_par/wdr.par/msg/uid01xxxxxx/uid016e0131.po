msgctxt "6980"
msgid "ちぇっちぇ、なんだよなんだよー！"
msgstr "Hey, what is it!"

msgctxt "7013"
msgid "みんなしてオレのことのけ者にしてさあ！"
msgstr "Let's all make me an outcast!"

msgctxt "7052"
msgid "オレだって……　オレだって……"
msgstr "Even me ... Even me ..."

msgctxt "7083"
msgid "オレだってさあ！"
msgstr "Come on me!"

msgctxt "7100"
msgid "…………"
msgstr ""

msgctxt "7109"
msgid "<!empty>"
msgstr ""

msgctxt "7110"
msgid "ひゃ、こ……　殺さないで！"
msgstr "Hey, this ... don't kill me!"

msgctxt "7137"
msgid "……お前、ひとをなんだと思ってるんだ？"
msgstr ""

msgctxt "7176"
msgid "あ、ご、ごごごめんなさい……"
msgstr "Oh, I'm sorry ..."

msgctxt "7205"
msgid "“オレだって”……なんだ？"
msgstr ""

msgctxt "7232"
msgid "えっ……"
msgstr "eh……"

msgctxt "7241"
msgid ""
"何かひとりで言っていただろう。\n"
"“オレだって”……　と。"
msgstr ""
"I would have said something alone. \"Even\n"
"I\" ..."

msgctxt "7298"
msgid "……あっ！"
msgstr ""

msgctxt "7309"
msgid ""
"……言いたくないならいい。\n"
"邪魔したな。"
msgstr ""

msgctxt "7350"
msgid "<!empty>"
msgstr ""

msgctxt "7351"
msgid "オレだって……　みんなと遊びたいんだ……！"
msgstr ""
"Even I ... I want to play with everyone\n"
"...!"

