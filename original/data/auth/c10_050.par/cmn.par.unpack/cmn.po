msgctxt "234060"
msgid "どや？　誰かおるか？"
msgstr ""

msgctxt "234204"
msgid "誰？　誰かおるの？"
msgstr ""

msgctxt "234348"
msgid "俺だ　おりょう"
msgstr ""

msgctxt "234492"
msgid ""
"一（はじめ）さん？\n"
"そんなとこで何してんの！？"
msgstr ""

msgctxt "234636"
msgid "聞いてくれ"
msgstr ""

msgctxt "234780"
msgid "今この宿を新選組が囲んでいる"
msgstr ""

msgctxt "234924"
msgid "え？"
msgstr ""

msgctxt "235068"
msgid "「坂本龍馬」って客が来ているな？"
msgstr ""

msgctxt "235212"
msgid "二階の客間か？"
msgstr ""

msgctxt "235356"
msgid "せやけど　何やの？"
msgstr ""

msgctxt "235500"
msgid "もうすぐ新選組がここへ突入してくる"
msgstr ""

msgctxt "235644"
msgid ""
"お前は静かに女将さんたちを連れて\n"
"寺田屋から逃げろ"
msgstr ""

msgctxt "235788"
msgid "坂本さんを　新選組が？"
msgstr ""

msgctxt "235932"
msgid "詳しい話は後だ　急げ"
msgstr ""

msgctxt "236076"
msgid "行ったようやな"
msgstr ""

msgctxt "236220"
msgid "よし　そしたら行こうか一（はじめ）ちゃん"
msgstr ""

msgctxt "236364"
msgid "お逃げください！　坂本様！！"
msgstr ""

msgctxt "236508"
msgid ""
"新選組が来ます！\n"
"早う　ここから逃げて！"
msgstr ""

msgctxt "236652"
msgid ""
"あれはおりょうの声\n"
"どういうことだ！？"
msgstr ""

msgctxt "236796"
msgid "迷うてるヒマはない"
msgstr ""

msgctxt "236940"
msgid "もう行くしかないで！"
msgstr ""

msgctxt "237084"
msgid "新選組じゃあ！"
msgstr ""

msgctxt "237228"
msgid "踏み込んできよった！"
msgstr ""

msgctxt "237372"
msgid "オモロなってきたやないか"
msgstr ""

msgctxt "237516"
msgid "行くで　一（はじめ）ちゃん！"
msgstr ""

msgctxt "237660"
msgid "ああ！"
msgstr ""
