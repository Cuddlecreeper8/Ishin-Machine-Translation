msgctxt "195168"
msgid "勝負はついた"
msgstr ""

msgctxt "195312"
msgid "こいつは死んだも同然だ"
msgstr ""

msgctxt "195456"
msgid "何をしてるんだ　お前！？"
msgstr ""

msgctxt "195600"
msgid "言ったじゃないですか"
msgstr ""

msgctxt "195744"
msgid "どちらかが死ぬまで終わらないと"
msgstr ""

msgctxt "195888"
msgid "だが"
msgstr ""

msgctxt "196032"
msgid ""
"ご安心ください　この男\n"
"どちらにしても死ぬ予定だったんです"
msgstr ""

msgctxt "196176"
msgid "先日　組から脱走を図りましてね"
msgstr ""

msgctxt "196320"
msgid "法度により処刑することになっていたんですよ"
msgstr ""

msgctxt "196464"
msgid "何だと？"
msgstr ""

msgctxt "196608"
msgid ""
"ですがただ殺すより\n"
"あなたを試す道具に使えると思いましてね"
msgstr ""

msgctxt "196752"
msgid "彼も天念理心流の使い手でしたから"
msgstr ""

msgctxt "196896"
msgid "誰なんだ　その男は"
msgstr ""

msgctxt "197040"
msgid "総長兼　三番隊隊長……"
msgstr ""

msgctxt "197184"
msgid "山南敬助"
msgstr ""

msgctxt "197328"
msgid "私にとっても　試衛館道場以来の友人です"
msgstr ""

msgctxt "197472"
msgid ""
"彼には　もしあなたを殺すことが出来れば\n"
"命だけは助けてやると言ってありましてね"
msgstr ""

msgctxt "197616"
msgid ""
"ですからあなたは　命がけで戦った\n"
"山南に勝ったということになります"
msgstr ""

msgctxt "197760"
msgid "そうやって俺を試した目的はなんだ？"
msgstr ""

msgctxt "197904"
msgid ""
"いや　実はあなたに折り入って\n"
"引き受けてもらいたい仕事がありましてね"
msgstr ""

msgctxt "198048"
msgid "何なんだ　それは"
msgstr ""

msgctxt "198192"
msgid "この山南の穴……"
msgstr ""

msgctxt "198336"
msgid "三番隊の隊長を引き受けてもらいたい"
msgstr ""

msgctxt "198480"
msgid "俺に　隊長を？"
msgstr ""

msgctxt "198624"
msgid "さ　無粋な話はここまでだ"
msgstr ""

msgctxt "198768"
msgid ""
"席を変えて　新選組の未来について\n"
"語り明かしませんか？"
msgstr ""

msgctxt "198912"
msgid "斎藤一君"
msgstr ""
