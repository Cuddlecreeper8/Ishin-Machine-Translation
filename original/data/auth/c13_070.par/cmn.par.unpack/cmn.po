msgctxt "106004"
msgid "お　西郷さんも来てたんですか"
msgstr ""

msgctxt "106148"
msgid ""
"罠かもしれないのに\n"
"さっすが　怖いもの知らずですねえ"
msgstr ""

msgctxt "106292"
msgid "二人の坂本龍馬　おもろそうな話やないか"
msgstr ""

msgctxt "106436"
msgid "そういうお前も　興味あるんとちゃうか？"
msgstr ""

msgctxt "106580"
msgid "ええ　興味津々って感じで"
msgstr ""

msgctxt "106724"
msgid ""
"……って　飲んで大丈夫なんですか\n"
"何が起きるか分かりませんよ？"
msgstr ""

msgctxt "106868"
msgid ""
"なら尚のこと飲まんとな\n"
"お前も飲めや"
msgstr ""

msgctxt "107012"
msgid "せっかくだけど遠慮しときます"
msgstr ""

msgctxt "107156"
msgid ""
"昨日の晩に俺の女が\n"
"ちょっとした騒動に巻き込まれちゃいましてね"
msgstr ""

msgctxt "107300"
msgid "一晩中介抱していたもんだから　体調が悪くて"
msgstr ""

msgctxt "107444"
msgid "集まってるな"
msgstr ""

msgctxt "107588"
msgid "どうも"
msgstr ""

msgctxt "107732"
msgid "どうなんや　“坂本龍馬”は来るんか"
msgstr ""

msgctxt "107876"
msgid "さあな　だが来るとすれば"
msgstr ""

msgctxt "108020"
msgid "そろそろなはずなんだが"
msgstr ""

msgctxt "108164"
msgid "お前が……"
msgstr ""
