msgid ""
"くくく……\n"
"こりゃ、ごっつい手になりそうや……"
msgstr ""

msgctxt "3167"
msgid "<!empty>"
msgstr ""

msgctxt "3168"
msgid ""
"おう、一（はじめ）ちゃんやないか。\n"
"お前も一緒にどや？"
msgstr ""

msgctxt "3223"
msgid ""
"いや……\n"
"だが、なんでそいつら服を脱いでいるんだ？"
msgstr ""

msgctxt "3274"
msgid "<!empty>"
msgstr ""

msgctxt "3275"
msgid "もう払う金がないいうからなぁ。"
msgstr ""

msgctxt "3306"
msgid ""
"服を質にとったんや。\n"
"ふんどしもいったろやないか。なあ？"
msgstr ""

msgctxt "3363"
msgid ""
"くくく……\n"
"これ以上負けたらこいつら武田はんに\n"
"何されるんやろなぁ？　どない思う？"
msgstr ""

msgctxt "3446"
msgid "そ、そんな……"
msgstr ""

msgctxt "3461"
msgid "…………"
msgstr ""

msgctxt "3470"
msgid ""
"なんや、一（はじめ）ちゃん？\n"
"何黙っとんねや？"
msgstr ""

msgctxt "3517"
msgid ""
"いや……　悪いが、局長を探している。\n"
"どこに行けば会えるんだ？"
msgstr ""

msgctxt "3580"
msgid ""
"さあなあ。\n"
"知らんわ、そんなもん。"
msgstr ""

msgctxt "3615"
msgid ""
"おう、気ぃが散るで。\n"
"用が済んだらさっさと行けや。"
msgstr ""

msgctxt "3666"
msgid "それとも、われも裸になりたいんか？"
msgstr ""

msgctxt "3701"
msgid "いや……　邪魔したな。"
msgstr ""

