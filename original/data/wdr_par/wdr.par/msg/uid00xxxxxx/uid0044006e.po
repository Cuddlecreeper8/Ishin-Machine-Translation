msgid ""
"ひ、ひいっ……\n"
"ば、ばけもんか、こいつ……"
msgstr ""

msgctxt "5699"
msgid "まだやるのか……？"
msgstr ""

msgctxt "5718"
msgid "も……　も……"
msgstr ""

msgctxt "5733"
msgid "もうやりませぇぇぇん！"
msgstr ""

