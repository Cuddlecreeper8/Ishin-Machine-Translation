msgctxt "2224"
msgid "まずは親を決めるのじゃ。"
msgstr ""

msgctxt "2249"
msgid "ワシが親じゃな……"
msgstr ""

msgctxt "2268"
msgid "いかほど賭けるのじゃ？"
msgstr ""

msgctxt "2291"
msgid "それじゃ、始めようか！"
msgstr ""

msgctxt "2314"
msgid "ワシの番じゃな……"
msgstr ""

msgctxt "2333"
msgid "（この台詞が出たら駄目）"
msgstr ""

msgctxt "2358"
msgid "精算するぞ……"
msgstr ""

msgctxt "2373"
msgid "お主が親じゃ……"
msgstr ""

msgctxt "2390"
msgid "親の交代じゃ……"
msgstr ""

msgctxt "2407"
msgid ""
"次に行くぞ！\n"
"もうひと勝負……どうじゃ？"
msgstr ""

msgctxt "2448"
msgid ""
"ぐぅ……これまでじゃ。\n"
"あとは、しかばねになるだけじゃ。"
msgstr ""

msgctxt "2505"
msgid ""
"ふん……これまでのようじゃな。\n"
"精進して出直すがよい……"
msgstr ""

msgctxt "2562"
msgid "親の即精算メッセージ。"
msgstr ""

msgctxt "2585"
msgid ""
"親の奇の見は\n"
"３として扱うのじゃ……"
msgstr ""

msgctxt "2622"
msgid ""
"最初の子が出した奇の見は\n"
"３として扱うのじゃ……"
msgstr ""

msgctxt "2671"
msgid ""
"奇の見は一つ前の子と\n"
"同じ出目に変化するのじゃ……"
msgstr ""

msgctxt "2722"
msgid ""
"親の偶の音は\n"
"４として扱うのじゃ……"
msgstr ""

msgctxt "2759"
msgid ""
"偶の音は一つ後の子と\n"
"同じ出目に変化するのじゃ……"
msgstr ""

msgctxt "2810"
msgid ""
"最後の子が出した偶の音は\n"
"４として扱うのじゃ……"
msgstr ""

msgctxt "2859"
msgid ""
"偶の音は、後ろの子と\n"
"同じ出目になるのじゃ。"
msgstr ""

msgctxt "2904"
msgid "偶の音のあとに奇の見が出たのじゃ。"
msgstr ""

msgctxt "2939"
msgid "ようし、ムサシじゃ！"
msgstr ""

msgctxt "2964"
msgid "死に神の市"
msgstr ""
