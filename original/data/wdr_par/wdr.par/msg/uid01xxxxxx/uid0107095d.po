msgctxt "2176"
msgid "まずは親決めや。"
msgstr ""

msgctxt "2193"
msgid "わしが親や。"
msgstr ""

msgctxt "2206"
msgid "さあ、いくら賭けんのや？"
msgstr ""

msgctxt "2231"
msgid "さあ、始めまっか！"
msgstr ""

msgctxt "2250"
msgid "わしの番やな。"
msgstr ""

msgctxt "2265"
msgid "（この台詞が出たら駄目）"
msgstr ""

msgctxt "2290"
msgid "精算や。"
msgstr ""

msgctxt "2299"
msgid "あんたが親やろ？"
msgstr ""

msgctxt "2316"
msgid "親の交代や。"
msgstr ""

msgctxt "2329"
msgid ""
"また、やりたいわ。\n"
"とことん勝負や！"
msgstr ""

msgctxt "2366"
msgid ""
"タネ切れや。\n"
"帰らせてもらうわ。"
msgstr ""

msgctxt "2399"
msgid ""
"タネ切れでっか。\n"
"出直して来いや。"
msgstr ""

msgctxt "2434"
msgid "親の即精算メッセージ。"
msgstr ""

msgctxt "2457"
msgid "親の奇の見は３や……"
msgstr ""

msgctxt "2478"
msgid ""
"最初の子が出した奇の見は\n"
"３や……"
msgstr ""

msgctxt "2513"
msgid ""
"奇の見は一つ前の子と\n"
"同じ出目や……"
msgstr ""

msgctxt "2550"
msgid "親の偶の音は４や……"
msgstr ""

msgctxt "2571"
msgid ""
"偶の音は一つ後の子と\n"
"同じ出目や……"
msgstr ""

msgctxt "2608"
msgid ""
"最後の子が出した偶の音は\n"
"４や……"
msgstr ""

msgctxt "2643"
msgid ""
"偶の音は、後ろの子と\n"
"同じ出目や。"
msgstr ""

msgctxt "2678"
msgid "偶の音のあとに奇の見が出たわ！"
msgstr ""

msgctxt "2709"
msgid "ムサシだわ！"
msgstr ""

msgctxt "2728"
msgid "胸熱な勘蔵"
msgstr ""
