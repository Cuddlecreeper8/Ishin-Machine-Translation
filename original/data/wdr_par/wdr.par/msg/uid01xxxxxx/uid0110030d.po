msgctxt "1060"
msgid ""
"待てや……\n"
"半端な腕で、ヤンチャすんなや。"
msgstr "Wait ... With a half-hearted arm, Yancha-san."

msgctxt "1103"
msgid ""
"待てや……\n"
"まさかイカサマしてへんやろな？"
msgstr "Wait ... Isn't it really crazy?"

msgctxt "1146"
msgid "フッ……　まあええやろ。"
msgstr "Huh ... well, yeah."

msgctxt "1171"
msgid "……"
msgstr ""

msgctxt "1176"
msgid ""
"（んん？\n"
"　全く動じてへんな……　勘違いか？）"
msgstr ""
"(Hmm? It's completely unmoving ... Is it\n"
"a misunderstanding?)"

msgctxt "1223"
msgid ""
"（遥ちゃんは丁や言うとる……\n"
"　さて、どうするかのう）"
msgstr ""
"(Haruka-chan tells me ... Well, what should\n"
"I do?)"

msgctxt "1278"
msgid ""
"（遥ちゃんは半や言うとる……\n"
"　さて、どうするかのう）"
msgstr ""
"(Haruka-chan says half ... Well, what should\n"
"I do?)"

msgctxt "1333"
msgid "（タネが尽きてもうた……）"
msgstr "(I've run out of seeds ...)"

msgctxt "1360"
msgid "（アイテムの効果が切れたようやな……）"
msgstr ""
"(It seems that the effect of the item has\n"
"expired ...)"

msgctxt "1404"
msgid "冴島"
msgstr "Saejima"
