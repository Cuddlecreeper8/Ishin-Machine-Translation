msgctxt "34068"
msgid ""
"斎藤殿ー！\n"
"ごめんくだされー！　斎藤殿ー！"
msgstr ""

msgctxt "34111"
msgid "（……来客か？）"
msgstr ""

msgctxt "34128"
msgid "……誰だ？"
msgstr ""

msgctxt "34139"
msgid "<!empty>"
msgstr ""

