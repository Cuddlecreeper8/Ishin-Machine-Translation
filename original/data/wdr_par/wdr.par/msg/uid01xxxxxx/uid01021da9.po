msgctxt "8407"
msgid "おっそいでぇ。一（はじめ）ちゃん。"
msgstr ""

msgctxt "8442"
msgid "沖田……　何か用か。"
msgstr ""

msgctxt "8463"
msgid ""
"ちょっとエエ話があんねん。\n"
"一（はじめ）ちゃんにだけのとっておきや。"
msgstr ""

msgctxt "8532"
msgid "<!empty>"
msgstr ""

msgctxt "8533"
msgid "……"
msgstr ""

msgctxt "8538"
msgid ""
"何やその顔。ま、ええわ。\n"
"あとで土蔵に来てもらおか。"
msgstr ""

msgctxt "8591"
msgid "……ここじゃマズイのか？"
msgstr ""

msgctxt "8616"
msgid ""
"２人っきりで話がしたいんや。\n"
"ワシと一（はじめ）ちゃんとで……　ほな。"
msgstr ""

msgctxt "8687"
msgid "<!empty>"
msgstr ""

msgctxt "8688"
msgid "（沖田……　何のつもりだ？）"
msgstr ""

msgctxt "8717"
msgid ""
"（まあ、気が向いたら話を聞いてみるか。\n"
"　土蔵で待っていると言っていたな）"
msgstr ""

