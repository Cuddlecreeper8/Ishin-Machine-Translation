msgctxt "6980"
msgid "ちぇっちぇ、なんだよなんだよー！"
msgstr ""

msgctxt "7013"
msgid "みんなしてオレのことのけ者にしてさあ！"
msgstr ""

msgctxt "7052"
msgid "オレだって……　オレだって……"
msgstr ""

msgctxt "7083"
msgid "オレだってさあ！"
msgstr ""

msgctxt "7100"
msgid "…………"
msgstr ""

msgctxt "7109"
msgid "<!empty>"
msgstr ""

msgctxt "7110"
msgid "ひゃ、こ……　殺さないで！"
msgstr ""

msgctxt "7137"
msgid "……お前、ひとをなんだと思ってるんだ？"
msgstr ""

msgctxt "7176"
msgid "あ、ご、ごごごめんなさい……"
msgstr ""

msgctxt "7205"
msgid "“オレだって”……なんだ？"
msgstr ""

msgctxt "7232"
msgid "えっ……"
msgstr ""

msgctxt "7241"
msgid ""
"何かひとりで言っていただろう。\n"
"“オレだって”……　と。"
msgstr ""

msgctxt "7298"
msgid "……あっ！"
msgstr ""

msgctxt "7309"
msgid ""
"……言いたくないならいい。\n"
"邪魔したな。"
msgstr ""

msgctxt "7350"
msgid "<!empty>"
msgstr ""

msgctxt "7351"
msgid "オレだって……　みんなと遊びたいんだ……！"
msgstr ""

msgctxt "7394"
msgid "<!empty>"
msgstr ""

msgctxt "7395"
msgid "<!empty>"
msgstr ""

msgctxt "7396"
msgid "…………"
msgstr ""

msgctxt "7405"
msgid "<!empty>"
msgstr ""

