msgctxt "11832"
msgid "……満足したか？"
msgstr ""

msgctxt "11849"
msgid "斎藤よ……"
msgstr ""

msgctxt "11860"
msgid "<!empty>"
msgstr ""

msgctxt "11861"
msgid ""
"……感動じゃ！　斎藤！\n"
"お前は舞踊の天才じゃあー！"
msgstr ""

msgctxt "11912"
msgid "そ、そうか……？"
msgstr ""

msgctxt "11929"
msgid ""
"あぁ、わしより上手いんじゃないか!?\n"
"すごいのう！　お前、踊りで食っていけるぞ！"
msgstr ""

msgctxt "12008"
msgid ""
"（うーむ、やはりどっちが接待されてるか\n"
"　わからなくなってくるな……）"
msgstr ""

msgctxt "12079"
msgid ""
"さぁ、次の踊りじゃ！\n"
"斎藤よ、もっと踊ってくれ！"
msgstr ""

msgctxt "12128"
msgid "何？　まだ踊るのか？"
msgstr ""

msgctxt "12149"
msgid "当然じゃ。わし、もっと斎藤の踊り見たい！"
msgstr ""

msgctxt "12190"
msgid "うーん、しかたないな……"
msgstr ""

msgctxt "12215"
msgid ""
"……お前、踊るの下手じゃのう！\n"
"がっはっは！"
msgstr ""

msgctxt "12260"
msgid "……すまん。"
msgstr ""

msgctxt "12273"
msgid ""
"そう落ち込むな。見たところ、\n"
"体を動かすこと自体は得意なようじゃからな。\n"
"練習すればすぐ上手くなるぞ！"
msgstr ""

msgctxt "12376"
msgid ""
"（うーむ、やはりどっちが接待されてるか\n"
"　わからなくなってくるな……）"
msgstr ""

msgctxt "12447"
msgid ""
"さぁ、次の踊りじゃ！\n"
"斎藤よ、もっと踊ってくれ！"
msgstr ""

msgctxt "12496"
msgid "何？　まだ踊るのか？"
msgstr ""

msgctxt "12517"
msgid ""
"当然じゃ。練習すれば上手くなると言っただろう！\n"
"上手くなるまで踊りの稽古じゃ！"
msgstr ""

msgctxt "12596"
msgid "むう……"
msgstr ""

msgctxt "12605"
msgid "<!empty>"
msgstr ""

msgctxt "12606"
msgid "<!empty>"
msgstr ""

msgctxt "12607"
msgid "<!empty>"
msgstr ""

msgctxt "12608"
msgid "<!empty>"
msgstr ""

msgctxt "12609"
msgid ""
"はぁ……　はぁ……\n"
"踊り疲れた……"
msgstr ""

msgctxt "12644"
msgid ""
"なんじゃ斎藤！　もうへばったのか！\n"
"だらしないのう！　がっはっは！"
msgstr ""

msgctxt "12711"
msgid "さすがにもう踊れんぞ……"
msgstr ""

msgctxt "12736"
msgid "そうか、結構結構……"
msgstr ""

msgctxt "12757"
msgid "む……　むむぅぅぅーっ!?"
msgstr ""

msgctxt "12782"
msgid "じ、爺さん!?　もしかしてまた……"
msgstr ""

msgctxt "12815"
msgid ""
"うむ！　斎藤の踊りを見ておったら、\n"
"突然、創作意欲がとめどなくあふれ出てきたわい！"
msgstr ""

msgctxt "12898"
msgid "俺の踊りで創作意欲が……"
msgstr ""

msgctxt "12923"
msgid ""
"ふぬぉぉぉ……　これはまた手に神が宿る兆し！\n"
"すぐに紙と筆を用意せい！　今すぐじゃ！"
msgstr ""

msgctxt "13008"
msgid "また神が!?　わ、わかった。"
msgstr ""

