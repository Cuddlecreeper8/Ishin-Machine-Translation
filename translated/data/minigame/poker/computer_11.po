msgid "もしかしたら勝てるかも"
msgstr "Maybe I can win"

msgctxt "BLUFF_MESSAGE_2"
msgid "まだ安心はでけへんな"
msgstr "I'm still not relieved"

msgctxt "BLUFF_MESSAGE_3"
msgid "最後まで油断はせぇへんで"
msgstr "Stay alert until the end"

msgctxt "GAME_MESSAGE_0"
msgid "いらっしゃーい！"
msgstr "welcome!"

msgctxt "GAME_MESSAGE_1"
msgid "軍資金が底を突いたな"
msgstr "Military funds have run out"

msgctxt "RITIRE_MESSAGE_0"
msgid "破産せんように計算せな"
msgstr "Do not calculate to go bankrupt"

msgctxt "RITIRE_MESSAGE_1"
msgid "勝負してくれて、ありがとさん"
msgstr "Thank you for playing"
