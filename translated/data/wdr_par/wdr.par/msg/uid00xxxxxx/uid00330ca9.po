msgstr "Konpeito"

msgctxt "17771"
msgid "草履"
msgstr "Zori"

msgctxt "17776"
msgid "笛"
msgstr "flute"

msgctxt "17869"
msgid "どん・きほーての店員との絆"
msgstr "Bonds with Don Quijote clerk"
