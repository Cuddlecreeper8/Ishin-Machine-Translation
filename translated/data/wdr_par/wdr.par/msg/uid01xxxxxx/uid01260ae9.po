msgctxt "3511"
msgid ""
"（この先が最後の部屋のようだ。\n"
"　このまま入るか？）"
msgstr "(This seems to be the last room. Will you enter as it is?)"

msgctxt "3582"
msgid "志半ばで……　む、無念……"
msgstr "In the middle of my ambition ..."

msgctxt "3609"
msgid "くそっ……　つ、強い……"
msgstr "Damn ... one, strong ..."

msgctxt "3634"
msgid "と、徳川の犬め……　覚えておけ……"
msgstr "Tokugawa's dog ... Remember ..."

msgctxt "3669"
msgid "これで終わりだと……　思うなよ……"
msgstr "I think this is the end ... I don't think ..."

msgctxt "3704"
msgid "し、新選組め……"
msgstr "And the new selection ..."

msgctxt "3721"
msgid "御館様……　すいません……"
msgstr "Mr. Gokan ... I'm sorry ..."

msgctxt "3748"
msgid "お……　御館様……"
msgstr "Oh …… Mr. Gokan ……"

msgctxt "3767"
msgid "ぐ……　ぐはっ……"
msgstr "Gu ... Guha ..."

msgctxt "3786"
msgid "くそっ……　ここまでか……"
msgstr "Damn ... up to here ..."

msgctxt "3813"
msgid "（任務は片付いた、屯所へ戻るか？）"
msgstr "(The mission has been completed, will you return to the tunsho?)"

msgctxt "4015"
msgid "入る"
msgstr "enter"

msgctxt "4020"
msgid "後にする"
msgstr "Later"

msgctxt "4035"
msgid "残党"
msgstr "Remnants"

msgctxt "4040"
msgid "戻らない"
msgstr "Dont return"

msgctxt "4049"
msgid "屯所に戻る"
msgstr "Return to tunsho"
