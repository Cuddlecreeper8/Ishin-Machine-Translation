msgid "後にする"
msgstr "Later"

msgctxt "1975"
msgid "四天王"
msgstr "Four heavenly kings"

msgctxt "1982"
msgid "戻らない"
msgstr "Dont return"

msgctxt "1991"
msgid "屯所に戻る"
msgstr "Return to tunsho"
