msgctxt "7641"
msgid "おいなりさん、１０貫ちょうだい。"
msgstr "Inari-san, please give me 10 pieces."

msgctxt "7674"
msgid "あいよ！　次の方、ご注文どうぞ！"
msgstr "Aiyo! Next person, please order!"

msgctxt "7707"
msgid "俺は２０貫くれ！"
msgstr "I'll pierce 20!"

msgctxt "7724"
msgid "あい、少々お待ちを！"
msgstr "Ai, wait a minute!"

msgctxt "7745"
msgid ""
"（新しくいなり寿司屋ができたのか……\n"
"　それにしても、すごく繁盛しているな）"
msgstr ""
"(Is there a new Inari sushi restaurant ...\n"
"Anyway, it's very prosperous)"

