msgid "<!empty>"
msgstr ""

msgctxt "36452"
msgid "……あ、あの、お侍様！"
msgstr ""

msgctxt "36475"
msgid "あ、あなたは！　大丈夫でしたか、お嬢さん！"
msgstr ""

msgctxt "36518"
msgid ""
"え……　えぇ。\n"
"あの、私、急いでるんで……　もう行きますね。"
msgstr ""

msgctxt "36579"
msgid "え、あ、はい！　……どうぞ!!"
msgstr ""

