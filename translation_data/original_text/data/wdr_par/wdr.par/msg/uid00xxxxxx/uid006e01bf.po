msgid "い、痛いよー！　わーん！"
msgstr ""

msgctxt "36264"
msgid "なんだ、こいつ……"
msgstr ""

msgctxt "36283"
msgid "……"
msgstr ""

msgctxt "36288"
msgid ""
"わ、私せっかく手加減してあげたのに、\n"
"あなた達、ちょっと本気、だしすぎですよ！\n"
"わ、わーん！　痛いよー！"
msgstr ""

msgctxt "36393"
msgid "……おい、なんだかしらけちまったな。"
msgstr ""

msgctxt "36430"
msgid "……あぁ、行こうぜ。"
msgstr ""

