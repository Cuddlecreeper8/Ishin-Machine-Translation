msgid "<!empty>"
msgstr ""

msgctxt "11215"
msgid ""
"ねぇ、信じてよ！\n"
"本当に……　熊がいたんだってば！"
msgstr ""

msgctxt "11266"
msgid "（……何？　熊だと？）"
msgstr ""

msgctxt "11289"
