msgctxt "848996"
msgid "武市半平太"
msgstr ""

msgctxt "849140"
msgid "覚悟！！！！"
msgstr ""

msgctxt "849284"
msgid "龍馬……"
msgstr ""

msgctxt "849428"
msgid "だめだ！"
msgstr ""

msgctxt "849572"
msgid "斬れ龍馬！"
msgstr ""

msgctxt "849716"
msgid ""
"俺を斬って\n"
"坂本龍馬という名を取り戻すんだ！"
msgstr ""

msgctxt "849860"
msgid "あんた"
msgstr ""

msgctxt "850004"
msgid "言ったじゃないか"
msgstr ""

msgctxt "850148"
msgid "坂本龍馬は"
msgstr ""

msgctxt "850292"
msgid "近江屋で死んだと"
msgstr ""

msgctxt "850436"
msgid "だがあれは"
msgstr ""

msgctxt "850580"
msgid "どっちが正しいかなんて"
msgstr ""

msgctxt "850724"
msgid "俺はどうでもいい"
msgstr ""

msgctxt "850868"
msgid "それより"
msgstr ""

msgctxt "851012"
msgid "俺にとっては"
msgstr ""

msgctxt "851156"
msgid "親っさんや兄弟が大切なんだ"
msgstr ""

msgctxt "851300"
msgid "兄弟の言うとおりさ"
msgstr ""

msgctxt "851444"
msgid "俺はどこまで行っても"
msgstr ""

msgctxt "851588"
msgid "自分の手が届く世界しか守れねえ"
msgstr ""

msgctxt "851732"
msgid "その程度の器の男だ"
msgstr ""

msgctxt "851876"
msgid "その言葉を聞いて良くわかった"
msgstr ""

msgctxt "852020"
msgid ""
"むしろ俺の方が　時代を変えられるような\n"
"器じゃないってことがな"
msgstr ""

msgctxt "852164"
msgid "俺は力でこの国を変えようとした"
msgstr ""

msgctxt "852308"
msgid "だが"
msgstr ""

msgctxt "852452"
msgid "序列や階級を打ち破ろうとしていた俺が"
msgstr ""

msgctxt "852596"
msgid ""
"いつの間にか\n"
"序列と階級を利用し始めていた"
msgstr ""

msgctxt "852740"
msgid "そうなったら最後"
msgstr ""

msgctxt "852884"
msgid ""
"もっと大きな権力に\n"
"使われる立場になってしまった"
msgstr ""

msgctxt "853028"
msgid "今となっちゃ"
msgstr ""

msgctxt "853172"
msgid "親っさんの気持ちが痛いほどわかる"
msgstr ""

msgctxt "853316"
msgid "親っさんの"
msgstr ""

msgctxt "853460"
msgid "気持ち？"
msgstr ""

msgctxt "853604"
msgid "ああ"
msgstr ""

msgctxt "853748"
msgid "時代を変えるには"
msgstr ""

msgctxt "853892"
msgid ""
"非情さよりも\n"
"もっと大事なもんがあったってことさ"
msgstr ""

msgctxt "854036"
msgid "大事なもん？"
msgstr ""

msgctxt "854180"
msgid "愛さ"
msgstr ""

msgctxt "854324"
msgid "どんな苦しみがあろうと"
msgstr ""

msgctxt "854468"
msgid ""
"愛を最後まで捨てなかった人間だけが\n"
"世界を変える"
msgstr ""

msgctxt "854612"
msgid "１年前　土佐を抜ける時"
msgstr ""

msgctxt "854756"
msgid "お前は無条件に俺を守るという道を選んだ"
msgstr ""

msgctxt "854900"
msgid "非情な道を選んだはずの"
msgstr ""

msgctxt "855044"
msgid "俺の心は揺れた"
msgstr ""

msgctxt "855188"
msgid "お前の愛の前では"
msgstr ""

msgctxt "855332"
msgid "知恵や策略は通用しない"
msgstr ""

msgctxt "855476"
msgid "そう悟った"
msgstr ""

msgctxt "855620"
msgid "あの時既に"
msgstr ""

msgctxt "855764"
msgid "俺は負けていたんだ"
msgstr ""

msgctxt "855908"
msgid "そのことに気づくのが遅すぎたがな"
msgstr ""

msgctxt "856052"
msgid "あんたにだって"
msgstr ""

msgctxt "856196"
msgid "愛はあるさ"
msgstr ""

msgctxt "856340"
msgid "桂に"
msgstr ""

msgctxt "856484"
msgid "もしものときは俺のことを頼むって"
msgstr ""

msgctxt "856628"
msgid "言ってくれてたんだろ？"
msgstr ""

msgctxt "856772"
msgid "ずっとあんたは俺を守ってくれていた"
msgstr ""

msgctxt "856916"
msgid "ガキの頃からずっと"
msgstr ""

msgctxt "857060"
msgid "それがあんたの愛なんじゃないのか？"
msgstr ""

msgctxt "857204"
msgid "兄弟"
msgstr ""

msgctxt "857348"
msgid "龍馬"
msgstr ""

msgctxt "857492"
msgid "兄弟も俺も　一度死んだ"
msgstr ""

msgctxt "857636"
msgid "武市半平太は土佐で"
msgstr ""

msgctxt "857780"
msgid "坂本龍馬は　近江屋で死んだんだ"
msgstr ""

msgctxt "857924"
msgid "一度死んだ人間同士"
msgstr ""

msgctxt "858068"
msgid "もうこれ以上"
msgstr ""

msgctxt "858212"
msgid "血を流す必要はねえ"
msgstr ""

msgctxt "858356"
msgid "憎しみ合う必要もないんだ"
msgstr ""

msgctxt "858500"
msgid "だから俺は"
msgstr ""

msgctxt "858644"
msgid "あんたを許す"
msgstr ""

msgctxt "858788"
msgid "あんたは俺の兄弟なんだ"
msgstr ""

msgctxt "858932"
msgid "武市半平太"
msgstr ""

msgctxt "859076"
msgid "夜が明けるな"
msgstr ""

msgctxt "859220"
msgid "ああ"
msgstr ""

msgctxt "859364"
msgid "兄弟"
msgstr ""

msgctxt "859508"
msgid "さあ　行くとするか"
msgstr ""

msgctxt "859652"
msgid "テメエのケツくらいテメエで拭かなきゃ"
msgstr ""

msgctxt "859796"
msgid "死んだ親っさんに顔向けできねぇからな"
msgstr ""

msgctxt "859940"
msgid "何をする気だ"
msgstr ""

msgctxt "860084"
msgid "土佐をぶっ壊すんだよ"
msgstr ""

msgctxt "860228"
msgid "この巨大な武器と化した土佐を"
msgstr ""

msgctxt "860372"
msgid "山内容堂の手に渡す前にな"
msgstr ""

msgctxt "860516"
msgid "どういうことなんだ"
msgstr ""

msgctxt "860660"
msgid "俺は"
msgstr ""

msgctxt "860804"
msgid "お前が新選組に入った頃から"
msgstr ""

msgctxt "860948"
msgid "容堂に使われるようになっていた"
msgstr ""

msgctxt "861092"
msgid ""
"あの男は　俺や\n"
"幕府の勝麟太郎を使い"
msgstr ""

msgctxt "861236"
msgid ""
"イギリスから\n"
"武器や兵器弾薬を仕入れさせてたんだ"
msgstr ""

msgctxt "861380"
msgid "全ては"
msgstr ""

msgctxt "861524"
msgid "ある計画のためにな"
msgstr ""

msgctxt "861668"
msgid ""
"この国に\n"
"大戦争を起こすためか"
msgstr ""

msgctxt "861812"
msgid "いや違うんだ"
msgstr ""

msgctxt "861956"
msgid "山内容堂は"
msgstr ""

msgctxt "862100"
msgid "この国を……！"
msgstr ""

msgctxt "862244"
msgid "龍馬！！"
msgstr ""

msgctxt "862388"
msgid "兄弟！？"
msgstr ""

msgctxt "862532"
msgid "これだから郷士上がりは信用できんのだ"
msgstr ""

msgctxt "862676"
msgid "容堂……！"
msgstr ""

msgctxt "862820"
msgid "武市よ"
msgstr ""

msgctxt "862964"
msgid ""
"お前が裏切ることなど\n"
"端から想定内のことだ"
msgstr ""

msgctxt "863108"
msgid "お前の考えを見破れんようでは"
msgstr ""

msgctxt "863252"
msgid ""
"この国の全員を「騙す」ことなど\n"
"できないからなあ"
msgstr ""

msgctxt "863396"
msgid "騙すだと？"
msgstr ""

msgctxt "863540"
msgid "お前があの坂本龍馬か"
msgstr ""

msgctxt "863684"
msgid "いや　なかなかいい活躍だったぞ"
msgstr ""

msgctxt "863828"
msgid ""
"大政奉還\n"
"あれは想定外のことだったが"
msgstr ""

msgctxt "863972"
msgid "結果的には非常に助かった"
msgstr ""

msgctxt "864116"
msgid ""
"この国を傷モノにしないで\n"
"済んだのだからなあ"
msgstr ""

msgctxt "864260"
msgid "何を言ってる"
msgstr ""

msgctxt "864404"
msgid "あいつは"
msgstr ""

msgctxt "864548"
msgid "この国を"
msgstr ""

msgctxt "864692"
msgid "丸ごと売るつもりなんだ"
msgstr ""

msgctxt "864836"
msgid "イギリスに……！"
msgstr ""

msgctxt "864980"
msgid "何だと！？"
msgstr ""

msgctxt "865124"
msgid "言葉が汚いぞ武市"
msgstr ""

msgctxt "865268"
msgid "教えただろう　売るのではない"
msgstr ""

msgctxt "865412"
msgid "イギリスの一部になるだけだ"
msgstr ""

msgctxt "865556"
msgid "国としての体は守ったままでな"
msgstr ""

msgctxt "865700"
msgid "同じことだ"
msgstr ""

msgctxt "865844"
msgid "あんたが狙っているのは　国王……"
msgstr ""

msgctxt "865988"
msgid "帝になって代わる　そういうことだろ！"
msgstr ""

msgctxt "866132"
msgid "今後　この国ではワシ以外の万民は"
msgstr ""

msgctxt "866276"
msgid "平等にクズとなる"
msgstr ""

msgctxt "866420"
msgid ""
"つまり　お前らのいう\n"
"階級社会を壊してやろうというのだ"
msgstr ""

msgctxt "866564"
msgid "貴様"
msgstr ""

msgctxt "866708"
msgid "ふざけるな！"
msgstr ""

msgctxt "866852"
msgid ""
"近くイギリス艦隊を\n"
"この土佐に招き入れる手はずだ"
msgstr ""

msgctxt "866996"
msgid "成立したての京の新政権になす術はない"
msgstr ""

msgctxt "867140"
msgid ""
"日本は\n"
"イギリスの植民地となろう"
msgstr ""

msgctxt "867284"
msgid ""
"イギリス人というのは\n"
"実に合理的でなあ"
msgstr ""

msgctxt "867428"
msgid ""
"自分たちの利権を最大限引き出せる日本人を\n"
"ひそかに捜していた"
msgstr ""

msgctxt "867572"
msgid ""
"強い権力を持ちながら\n"
"国を売ることをも厭わない人間を"
msgstr ""

msgctxt "867716"
msgid "それが"
msgstr ""

msgctxt "867860"
msgid "このワシだ！"
msgstr ""

msgctxt "868004"
msgid "どうした？"
msgstr ""

msgctxt "868148"
msgid "お前もワシに跪くか？"
msgstr ""

msgctxt "868292"
msgid "今　土下座して請えば命は助けてやろう"
msgstr ""

msgctxt "868436"
msgid "雑兵の一人くらいにはしてやるぞ"
msgstr ""

msgctxt "868580"
msgid "嫌か"
msgstr ""

msgctxt "868724"
msgid "ならばこのワシ自ら"
msgstr ""

msgctxt "868868"
msgid "死を与えてやろう"
msgstr ""

msgctxt "869012"
msgid "おい！"
msgstr ""

msgctxt "869156"
msgid "何してるんだ龍馬"
msgstr ""

msgctxt "869300"
msgid "やめろ！"
msgstr ""

msgctxt "869444"
msgid "龍馬！！"
msgstr ""

msgctxt "869588"
msgid "化け物が……！"
msgstr ""

msgctxt "869732"
msgid "往生際が悪いぞ！！"
msgstr ""

msgctxt "869876"
msgid "よく目に焼き付けとけ"
msgstr ""

msgctxt "870020"
msgid "これが"
msgstr ""

msgctxt "870164"
msgid "国のため"
msgstr ""

msgctxt "870308"
msgid "愛のため"
msgstr ""

msgctxt "870452"
msgid "闘った男たちの"
msgstr ""

msgctxt "870596"
msgid "生き様だ"
msgstr ""

msgctxt "870740"
msgid "誰だ　貴様！？"
msgstr ""

msgctxt "870884"
msgid "往生せえや"
msgstr ""

msgctxt "871028"
msgid "ここがワレの死に場所や"
msgstr ""

msgctxt "871172"
msgid "安心しろ"
msgstr ""

msgctxt "871316"
msgid "独りで逝けとは言わない"
msgstr ""

msgctxt "871460"
msgid "私たちも地獄までお供しよう"
msgstr ""

msgctxt "871604"
msgid "頼む！　た　助けてくれ！！"
msgstr ""

msgctxt "871748"
msgid "き　君には　この土佐……"
msgstr ""

msgctxt "871892"
msgid ""
"いや　四国全土を与えよう！\n"
"だ　だから……"
msgstr ""

msgctxt "872036"
msgid "見逃してくれ！！"
msgstr ""

msgctxt "872180"
msgid "龍馬"
msgstr ""

msgctxt "872324"
msgid ""
"わ　ワシの話を良く聞くんだ！\n"
"坂本！"
msgstr ""

msgctxt "872468"
msgid "坂本龍馬！！！！"
msgstr ""

msgctxt "872612"
msgid "誰だそれは"
msgstr ""

msgctxt "872756"
msgid "そんな名前の男"
msgstr ""

msgctxt "872900"
msgid "知らねえな"
msgstr ""

msgctxt "873044"
msgid "え？"
msgstr ""

msgctxt "873188"
msgid "や　やめろ！"
msgstr ""

msgctxt "873332"
msgid "ワシを殺せば　この国はどうなる！"
msgstr ""

msgctxt "873476"
msgid "鎖国で弱りきった"
msgstr ""

msgctxt "873620"
msgid ""
"世間知らずの日本を\n"
"救えるのはワシだけだ！！"
msgstr ""

msgctxt "873764"
msgid "誰がてめえに"
msgstr ""

msgctxt "873908"
msgid "この国のケツ拭いてくれって頼んだ"
msgstr ""

msgctxt "874052"
msgid "この国の道を決めんのは"
msgstr ""

msgctxt "874196"
msgid "お前でも俺でもねえ"
msgstr ""

msgctxt "874340"
msgid "ここにいる誰でもな"
msgstr ""

msgctxt "874484"
msgid ""
"は　話にならない\n"
"何悠長なことを言っているんだ！！"
msgstr ""

msgctxt "874628"
msgid ""
"今　目の前に\n"
"イギリスの艦隊が迫っているんだぞ！？"
msgstr ""

msgctxt "874772"
msgid ""
"奴らとまともに闘って\n"
"勝てるとでも思っているのか！？"
msgstr ""

msgctxt "874916"
msgid "さあな"
msgstr ""

msgctxt "875060"
msgid "そんな事は"
msgstr ""

msgctxt "875204"
msgid "知ったこっちゃねえ"
msgstr ""

msgctxt "875348"
msgid "ば　バカか！？"
msgstr ""

msgctxt "875492"
msgid ""
"わ　ワシはお前ら郷士のような\n"
"その日暮らしの連中とは違うのだ！"
msgstr ""

msgctxt "875636"
msgid ""
"この国が生き残る道を\n"
"誰よりも考えている！"
msgstr ""

msgctxt "875780"
msgid "お前らは　ワシに従えばいいのだ！"
msgstr ""

msgctxt "875924"
msgid "そうだろ！！"
msgstr ""

msgctxt "876068"
msgid "言いたい事はそれだけか"
msgstr ""

msgctxt "876212"
msgid "あ……？"
msgstr ""

msgctxt "876356"
msgid "イギリスの艦隊がどうした"
msgstr ""

msgctxt "876500"
msgid "そんなんで日本は"
msgstr ""

msgctxt "876644"
msgid "滅びる国じゃねえ"
msgstr ""

msgctxt "876788"
msgid "お前はわかっちゃいない！"
msgstr ""

msgctxt "876932"
msgid "負けるのは目に見えているんだぞ！！"
msgstr ""

msgctxt "877076"
msgid "いいや"
msgstr ""

msgctxt "877220"
msgid "たとえ今負けたとしても"
msgstr ""

msgctxt "877364"
msgid "日本は何度でも立ち上がるさ"
msgstr ""

msgctxt "877508"
msgid "まだ見ぬ兄弟たちによってな"
msgstr ""

msgctxt "877652"
msgid "何だと？"
msgstr ""

msgctxt "877796"
msgid "俺らは兄弟たちに伝えなきゃならねえんだ"
msgstr ""

msgctxt "877940"
msgid "この国に生きるものにとって"
msgstr ""

msgctxt "878084"
msgid "本当に必要なもんを"
msgstr ""

msgctxt "878228"
msgid "この国には"
msgstr ""

msgctxt "878372"
msgid "まだ多くの血が流れるだろう"
msgstr ""

msgctxt "878516"
msgid "しかし土佐を出て"
msgstr ""

msgctxt "878660"
msgid "京で生き"
msgstr ""

msgctxt "878804"
msgid "初めて分かった"
msgstr ""

msgctxt "878948"
msgid "近藤や桂に西郷"
msgstr ""

msgctxt "879092"
msgid "それに"
msgstr ""

msgctxt "879236"
msgid "武市の兄弟"
msgstr ""

msgctxt "879380"
msgid "皆"
msgstr ""

msgctxt "879524"
msgid ""
"流れた血の先に\n"
"何があるのかを知っている"
msgstr ""

msgctxt "879668"
msgid "な　何を言っている……！？"
msgstr ""

msgctxt "879812"
msgid "人間ってのは間違う"
msgstr ""

msgctxt "879956"
msgid "間違ってばかりだ"
msgstr ""

msgctxt "880100"
msgid "でもな"
msgstr ""

msgctxt "880244"
msgid "それでも俺らは"
msgstr ""

msgctxt "880388"
msgid "進まなきゃならねえんだよ"
msgstr ""

msgctxt "880532"
msgid "この国の夜明けを信じてな"
msgstr ""

msgctxt "880676"
msgid "進むことを止めちまったら"
msgstr ""

msgctxt "880820"
msgid "託すべき未来もなくなっちまう"
msgstr ""

msgctxt "880964"
msgid "俺らの仕事は"
msgstr ""

msgctxt "881108"
msgid "生きていける世の中を作ることじゃねえ"
msgstr ""

msgctxt "881252"
msgid ""
"生きていてよかったと思える\n"
"国を作ることなんじゃねえのか！"
msgstr ""

msgctxt "881396"
msgid "よ　よせぇ！"
msgstr ""

msgctxt "881540"
msgid "この国の未来は"
msgstr ""

msgctxt "881684"
msgid "今はまだ顔も見たこともねえ"
msgstr ""

msgctxt "881828"
msgid "声を聞いたこともねえ兄弟たちが"
msgstr ""

msgctxt "881972"
msgid "きっと作ってくれる"
msgstr ""

msgctxt "882116"
msgid "一緒にあの世で"
msgstr ""

msgctxt "882260"
msgid "日本の夜明けを見ようじゃねえか"
msgstr ""

msgctxt "882404"
msgid "容堂さん"
msgstr ""

msgctxt "882548"
msgid "や　やめるんだ　やめるんだ"
msgstr ""

msgctxt "882692"
msgid "龍馬！！"
msgstr ""

msgctxt "882836"
msgid "日本の兄弟たちよ"
msgstr ""

msgctxt "882980"
msgid "また明日じゃ！！"
msgstr ""
