msgctxt "136532"
msgid "どうしてだろうな"
msgstr "I wonder why"

msgctxt "136676"
msgid "今日はやけに兄弟が"
msgstr "Today my brothers"

msgctxt "136820"
msgid "生き生きとしてみえるぜ"
msgstr "It looks alive"

msgctxt "136964"
msgid "ああ"
msgstr "Oh"

msgctxt "137108"
msgid "俺にも分からねえ"
msgstr "I don't even know"

msgctxt "137252"
msgid "お前とまた闘えると思うだけで心が躍る"
msgstr "I'm excited just to think that I can fight you again"

msgctxt "137396"
msgid "待ってる間に若返っちまったようだ"
msgstr "It seems that I was rejuvenated while I was waiting"

msgctxt "137540"
msgid "後悔は"
msgstr "Regret"

msgctxt "137684"
msgid "無しだぜ"
msgstr "No"

msgctxt "137828"
msgid "兄弟"
msgstr "brothers"

msgctxt "137972"
msgid "忘れちまったな"
msgstr "I forgot"

msgctxt "138116"
msgid "後悔なんて言葉は"
msgstr "The word regret"

msgctxt "138260"
msgid "来い"
msgstr "Come on"

msgctxt "138404"
msgid "龍馬！！！！"
msgstr "Ryoma! ! ! !"

msgctxt "138548"
msgid "行くぞ！"
msgstr "let's go!"

msgctxt "138692"
msgid "武市！！！！"
msgstr "Takeshi! !! !! !!"
