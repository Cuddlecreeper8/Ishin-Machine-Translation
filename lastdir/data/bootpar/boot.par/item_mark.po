msgid "ヒートゲージが溜まりやすい"
msgstr "Heat gauge tends to accumulate"

msgctxt "hint_5"
msgid "属性攻撃<Color:109,193,240,255>（青い印）<Color:Default>の効果上昇"
msgstr ""
"Attribute attack<Color:109,193,240,255> (Blue mark)<Color:Default> Effect "
"increase"

msgctxt "hint_6"
msgid "敵の量で攻撃力増加"
msgstr "Attack power increases with the amount of enemies"

msgctxt "hint_7"
msgid "連射性能上昇"
msgstr "Increased rate of fire performance"

msgctxt "hint_8"
msgid "連射性能が大きく上昇"
msgstr "Greatly improved rate of fire"

msgctxt "hint_9"
msgid "１度に２発弾丸を発射"
msgstr "Fire two bullets at a time"

msgctxt "hint_10"
msgid "格闘攻撃の威力増加"
msgstr "Increased power of fighting attacks"

msgctxt "hint_11"
msgid "格闘攻撃の威力が大きく増加"
msgstr "Greatly increased the power of combat attacks"

msgctxt "hint_12"
msgid "酔うと攻撃力とヒート上昇"
