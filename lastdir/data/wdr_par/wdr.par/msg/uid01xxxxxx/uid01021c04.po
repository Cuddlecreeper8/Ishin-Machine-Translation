msgctxt "4601"
msgid "おい、あれ見ろ。"
msgstr "Hey, look at that."

msgctxt "4618"
msgid "お……　ありゃあ……"
msgstr "Oh ... there is ..."

msgctxt "4639"
msgid "新選組だ……！"
msgstr "Shinsengumi ...!"

msgctxt "4654"
msgid "いかん、引き返そう。"
msgstr "Let's turn back."

msgctxt "4676"
msgid "チッ……"
msgstr "Chi ..."

msgctxt "4687"
msgid "お……　おんしは坂本!?"
msgstr "Oh ... I'm Sakamoto !?"

msgctxt "4710"
msgid ""
"やっぱりそうぜよ！　わしじゃ！\n"
"ほれ、土佐の島田弥平！"
msgstr "After all it is! I am! Look, Yahei Shimada from Tosa!"

msgctxt "4765"
msgid "おんし、京に来ちょったがかえ！"
msgstr "Oh, I came to Kyo!"

msgctxt "4797"
msgid "人違いだ。"
msgstr "It's a mistake."

msgctxt "4808"
msgid ""
"ちょ、待ちい、なに言うがじゃ“筆頭”！\n"
"脱藩してからどこにおったがじゃ！"
msgstr ""
"Wait a minute, what do you say, \"first\"! Where have you been since you "
"left the domain!"

msgctxt "4881"
msgid "!?　大声出すな。"
msgstr "!? Don't shout."

msgctxt "4899"
msgid ""
"あ、ありゃ、新選組ぜよ！\n"
"や、や、ヤバい！　逃げるがじゃ!!"
msgstr "Oh, there's a Shinsengumi! Somewhat dangerous! I'll run away !!"

msgctxt "4958"
msgid "逃げやがった！　おい捕まえろ！"
msgstr "I ran away! Catch it!"

msgctxt "4989"
msgid "チッ。"
msgstr "Chit."

msgctxt "5808"
msgid "怪しい浪人"
msgstr "Suspicious Ronin"

msgctxt "5838"
msgid "すねに傷持つ浪人"
msgstr "Ronin with a wound on the shin"

msgctxt "5946"
msgid "土佐浪人"
msgstr "Ronin Tosa"

msgctxt "6092"
msgid "新選組隊士"
msgstr "Shinsengumi"
