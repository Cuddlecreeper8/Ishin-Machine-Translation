msgctxt "2408"
msgid "何をしますか？"
msgstr "What are you doing?"

msgctxt "2424"
msgid "昼まで休むとするか。"
msgstr "Do you want to rest until noon?"

msgctxt "2446"
msgid "夕方まで休むとするか。"
msgstr "Do you want to rest until the evening?"

msgctxt "2470"
msgid "夜まで休むとするか。"
msgstr "Do you want to rest until night?"

msgctxt "2492"
msgid ""
"（準備は整った……\n"
"　出発まで休もう……）"
msgstr "(Ready ... Let's rest until departure ...)"

msgctxt "2537"
msgid "しばらく休みますか？"
msgstr "Do you want to rest for a while?"

msgctxt "2559"
msgid "翌朝まで休むとするか。"
msgstr "Do you want to rest until the next morning?"

msgctxt "2583"
msgid "夕方まで休むとするか。"
msgstr "Do you want to rest until the evening?"

msgctxt "2607"
msgid "夜まで休むとするか。"
msgstr "Do you want to rest until night?"

msgctxt "2656"
msgid "昼まで休む"
msgstr "Rest until noon"

msgctxt "2667"
msgid "夕方まで休む"
msgstr "Rest until evening"

msgctxt "2680"
msgid "夜まで休む"
msgstr "Rest till night"

msgctxt "2691"
msgid "近江屋へ向かう"
msgstr "Head to Omiya"

msgctxt "2706"
msgid "やめる"
msgstr "Stop"

msgctxt "2713"
msgid "龍馬"
msgstr "Ryoma"
