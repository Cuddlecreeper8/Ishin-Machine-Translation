msgctxt "7864"
msgid ""
"し、信じられん……\n"
"なんなんだ、こいつ……"
msgstr "I can't believe it ... what the heck ..."

msgctxt "7907"
msgid "まだやるか……？"
msgstr "Do you still do it ...?"

msgctxt "7924"
msgid "も……　も……"
msgstr "Peaches……"

msgctxt "7939"
msgid "もうやりませぇぇぇぇん！"
msgstr "I won't do it anymore!"

